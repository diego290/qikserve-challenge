{
  "name": "qikserve-challenge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:prd": "vite --mode production",
    "start:dev": "vite --mode development",
    "start:stubby": "concurrently \"vite --mode staging\" \"npm run stubs\"",
    "start:testing": "concurrently \"vite --mode testing\" \"npm run stubs\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage",
    "stubs": "stubby -s 8080 -d src/stubbs/stubby.json -w",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:e2e": "start-server-and-test start:testing http-get://localhost:5173 cy:open-e2e",
    "cy:test-e2e": "start-server-and-test start:testing http-get://localhost:5173 cy:run-e2e"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@reduxjs/toolkit": "^1.9.6",
    "axios": "^1.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-redux": "^8.1.2",
    "sass": "^1.70.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^19.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cypress/webpack-preprocessor": "^6.0.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-redux": "^7.1.27",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "stubby": "^5.1.0",
    "start-server-and-test": "^2.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-environment": "^1.1.3"
  }
}
